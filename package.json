    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "bundle": "react-native bundle --dev false --platform android --entry-file index.js --bundle-output ./android/app/src/main/assets/index.android.bundle --assets-dest ./android/app/src/main/res",
    "deploy-android-release": "cd android && ./gradlew clean && rm -rf ./app/src/main/res/drawable-* && ./gradlew assembleRelease --warning-mode all --stacktrace --info",
    "deploy-android-debug": "npm run bundle && cd android && ./gradlew clean && ./gradlew assembleDebug --warning-mode all --stacktrace --info"
  },
